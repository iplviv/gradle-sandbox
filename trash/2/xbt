ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:    displayValue: function(component) {
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:        var concCmp = component.getConcreteComponent();
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:        var value = concCmp.get("v.value");
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:        var displayValue = value;
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:        if (value) {
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:            var d = $A.localizationService.parseDateTimeUTC(value, "yyyy-MM-dd");
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:                var format = concCmp.get("v.format");
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:                var langLocale = concCmp.get("v.langLocale");
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:                    d = $A.localizationService.translateToOtherCalendar(d);
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:                    displayValue = $A.localizationService.formatDateUTC(d, format, langLocale);
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:                } catch (e) {
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:                    displayValue = e.message;
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:        /**This instance of the component variable was left in because in cases when we are extending inputDate,
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:         * getting the concreteComponent will give us the lowest hanging fruit, which does not include an 
ruau/ruau-components/src/main/components/ui/inputDate/inputDateHelper.js:         * element with an id of inputText. By leaving this variable in, it will work in both cases.
