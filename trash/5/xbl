ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                var title = node.get('v.item.title') || '' + node.get('v.title') || '';
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                    // This node is a match.
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                    var record = this.stack[this.stack.length - 1];
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                    record.match = true;
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:             * Add a node to our stack as we start to examine it.
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:            helper.SearchFilterVisitor.prototype.visit = function(node) {
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                // We begin our visit assuming no match.
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                this.stack.push({
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                    "match" : false
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:             * When we are done traversing the node, act on its match state and
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:             * possibly update the match state of its parent.
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:            helper.SearchFilterVisitor.prototype.endVisit = function(node) {
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                var record = this.stack.pop();
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                // TODO: this match logic can give a funky UX when we match a
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                $A.assert(node === record.node, "endVisit got an unexpected node");
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                if (!record.match) {
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                    // Collapse a node if it doesn't match
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                    node.set('v.expanded', false);
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                    var title = node.get('v.item.title') || '' + node.get('v.title') || '';
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                    // If a node matches then expand and show it.
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                    node.set('v.expanded', true);
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                    // This is not strictly necessary until we start hiding on non-matches.
ruau/ruau-components/src/nAmE/components/ruaudocs/searchFilterTree/searchFilterTreeHelper.js:                    node.set('v.hidden', false);
