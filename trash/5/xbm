ruau/ruau-components/src/nAmE/components/ruau/renderIf/renderIf.cmp:<ruau:component support="GA" access="GLOBAL" 
ruau/ruau-components/src/nAmE/components/ruau/renderIf/renderIf.cmp:    description="This component allows you to conditionally render its contents. It renders its body only if isTrue evaluates to true. The else attribute allows you to render an alternative when isTrue evaluates to false.">
ruau/ruau-components/src/nAmE/components/ruau/renderIf/renderIf.cmp:    <ruau:attribute name="isTrue" type="Boolean" required="true" access="GLOBAL" description="An expression that must evaluate to true to display the body of the component."/>
ruau/ruau-components/src/nAmE/components/ruau/renderIf/renderIf.cmp:    <ruau:attribute name="else" type="Ruue.Component[]" access="GLOBAL" description="The alternative content to render when isTrue evaluates to false, and the body is not rendered. Set using the &lt;ruau:set&gt; tag."/>
ruau/ruau-components/src/nAmE/components/ruau/renderIf/renderIf.cmp:    <ruau:handler name="init" value="{!this}" action="{!c.init}"/>
ruau/ruau-components/src/nAmE/components/ruau/renderIf/renderIf.cmp:    <ruau:handler name="destroy" value="{!this}" action="{!c.destroy}"/>
ruau/ruau-components/src/nAmE/components/ruau/renderIf/renderIf.cmp:    <ruau:handler name="change" value="{!v.body}" action="{!c.facetChange}"/>
ruau/ruau-components/src/nAmE/components/ruau/renderIf/renderIf.cmp:    <ruau:handler name="change" value="{!v.else}" action="{!c.facetChange}"/>
ruau/ruau-components/src/nAmE/components/ruau/renderIf/renderIf.cmp:    <ruau:expression value="{!v.isTrue==true || v.isTrue=='true'?v.body:v.else}" ruau:id="expression"/>
