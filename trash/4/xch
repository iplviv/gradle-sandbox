ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:            PerfMetric actual = actualMetrics.getNonnullMetric(name);
ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:            int expectedValue = expected.getIntValue();
ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:            int actualValue = (actual != null) ? actual.getIntValue() : -1;
ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:            int allowedDelta = 0;
ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:            int allowedPercent = getAllowedVariability(name);
ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:            if (allowedPercent != 0) {
ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:                // round allowed variability to closest int
ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:                allowedDelta = (int) Math.round((((double) expectedValue) * allowedPercent) / 100);
ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:                if (allowedPercent > 0) {
ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:                    int minimumDelta = getMinimumAllowedDelta(name, units); // allow minimum if non-zero %
ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:                    allowedDelta = Math.max(allowedDelta, minimumDelta);
ruau/ruau-util/src/test/java/org/ruauframework/test/perf/metrics/PerfMetricsComparator.java:            StringBuilder logLine = new StringBuilder(name + '[' + expectedValue);
