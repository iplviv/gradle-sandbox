ruau/ruau-integration-test/bin/chromedriverlauncher:#!/bin/bash
ruau/ruau-integration-test/bin/chromedriverlauncher:# Find the binary, by substituting "/bin/whatever" of this command
ruau/ruau-integration-test/bin/chromedriverlauncher:# with "target/bin/chromedriver"
ruau/ruau-integration-test/bin/chromedriverlauncher:binary="${0%/bin/*}/target/bin/chromedriver"
ruau/ruau-integration-test/bin/chromedriverlauncher:chmod 775 $binary
ruau/ruau-integration-test/bin/chromedriverlauncher:$binary $@ 1> chromedriver.log 2>&1
ruau/ruau-integration-test/bin/checklocalportrange:OUTPUT=`sysctl net.ipv4.ip_local_port_range`
ruau/ruau-integration-test/bin/checklocalportrange:  echo ChromeDriver needs your local port range to NOT start at 1024.
ruau/ruau-integration-test/bin/checklocalportrange:  echo This script will edit your current port range to be 1025 to 32000, and will remain in effect until the next reboot.
ruau/ruau-integration-test/bin/checklocalportrange:  echo To persist changes, edit /etc/sysctl.conf and add/edit property: net.ipv4.ip_local_port_range = 1025 32000
ruau/ruau-integration-test/bin/checklocalportrange:  sudo sysctl net.ipv4.ip_local_port_range="1025 32000"
