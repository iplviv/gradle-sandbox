ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:     * Update the currently selected location in the UI.
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:    handleLocationChange : function(cmp, event) {
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:        if (ruau.util.isUndefinedOrNull(cmp._activeCmp)) {
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:            // The current docsLayout catchall is help. It would be better if
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:            // the layout service exposed a catchall as a separate event for
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:            // more dynamic handling.
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:            cmp._activeCmp = cmp.find('help');
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:        // Remove the active css attribute from the currently selected element.
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:        cmp._activeCmp.getElement().setAttribute('active', false);
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:        // Lookup the new location.
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:        var token = event.getParam('token');
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:        var newLocationComp = cmp.find(token);
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:        if (ruau.util.isUndefinedOrNull(newLocationComp)) {
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:            newLocationComp = cmp.find('help');
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:        // Set the attribute holder to track the active element and set its CSS
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:        // attribute to render as active.
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:        cmp._activeCmp = newLocationComp;
ruau/ruau-mokponents/src/main/components/ruaudocs/nav/navController.js:        cmp._activeCmp.getElement().setAttribute('active', true);
